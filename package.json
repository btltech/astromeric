{
  "name": "astronumeric",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "VITE_API_URL=https://astronumeric-backend-production.up.railway.app vite build",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:ci": "eslint --ext .ts,.tsx --max-warnings 0 .",
    "lint:fix": "eslint --ext .ts,.tsx --fix .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "deploy:frontend": "npm run build:prod && npx wrangler pages deploy dist --branch main --compatibility-date 2024-01-01",
    "deploy:backend": "railway up",
    "deploy:backend:logs": "railway logs -f",
    "deploy:healthcheck": "curl -sf $VITE_API_URL/health || (echo 'Backend health check failed!' && exit 1)",
    "deploy:frontend:safe": "npm run deploy:check-env && npm run deploy:healthcheck && npm run deploy:frontend",
    "deploy:all": "npm run deploy:backend && npm run deploy:frontend:safe",
    "deploy": "npm run deploy:frontend"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@types/react-router-dom": "^5.3.3",
    "framer-motion": "^12.23.24",
    "html-to-image": "^1.11.13",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.4.0",
    "react-router-dom": "^7.9.6",
    "three": "0.160.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^27.2.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.34.5",
    "terser": "^5.44.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.15"
  }
}
